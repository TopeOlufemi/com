{
    "version": 2,
    "builds": [
        {
            "src": "verifs/index.html",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/images/(.*)",
            "dest": "/images/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/verifs/$1"
        }
    ]
} 